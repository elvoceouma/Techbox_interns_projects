<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="rental_management_view_form" model="ir.ui.view">
        <field name="name">rental.management.view.form</field>
        <field name="model">rental.management</field>
        <field name="arch" type="xml">
            <form string="Rental">
                <sheet>
                    <group>
                        <field name="name"></field>
                        <field name="house_number"></field>
                        <field name="house_type"></field>
                        <field name="email"></field>
                        <field name="phone_number"></field>
                        <field name="Rent_amount"></field>
                        <field name="date"></field>

                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="rental_management_view_tree" model="ir.ui.view">
        <field name="name">rental.management.view.tree</field>
        <field name="model">rental.management</field>
        <field name="arch" type="xml">
            <tree string="Rentals">
                <field name="name"></field>
                <field name="house_number"></field>
                <field name="house_type"></field>
                <field name="email"></field>
            </tree>
        </field>
    </record>

</odoo>
